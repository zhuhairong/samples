<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="trtcvoiceroom_btn_text">@color/trtcvoiceroom_text_color_disable</color>
    <color name="trtcvoiceroom_circle">#020714</color>
    <color name="trtcvoiceroom_color_black">#FF000000</color>
    <color name="trtcvoiceroom_color_text_blue">#FF0062E3</color>
    <color name="trtcvoiceroom_color_text_red">#FF4E42</color>
    <color name="trtcvoiceroom_color_transparent_black">#44777777</color>
    <color name="trtcvoiceroom_text_color_disable">#56749E</color>
    <color name="trtcvoiceroom_text_color_hint">#6B82A8</color>
    <color name="trtcvoiceroom_text_color_second">#EBF4FF</color>
    <color name="trtcvoiceroom_transparent">#00000000</color>
    <color name="white">#ffffff</color>
    <dimen name="large_image_left_margin">10dp</dimen>
    <dimen name="small_image_left_margin">5dp</dimen>
    <dimen name="small_image_size">30dp</dimen>
    <dimen name="text_size_large">18sp</dimen>
    <dimen name="text_size_middle">14sp</dimen>
    <dimen name="title_text_size">16sp</dimen>
    <string name="trtcvoiceroom_agree">同意</string>
    <string name="trtcvoiceroom_already_agree">已同意</string>
    <string formatted="false" name="trtcvoiceroom_main_title">%1s(%2d)</string>
    <string formatted="false" name="trtcvoiceroom_numer_format">%d人在看</string>
    <string formatted="false" name="trtcvoiceroom_room_title">房间号: %d</string>
    <string name="trtcvoiceroom_send">发送</string>
    <string name="trtcvoiceroom_warning_not_empty">"输入不能为空!"</string>
    <style name="ChatTheme" parent="Theme.AppCompat.NoActionBar">
        <item name="android:textColor">@color/white</item>
        <!--应用的背景色，popMenu的背景默认使用该颜色-->
    </style>
    <style name="TRTCVoiceRoomButtonStyle">
        <item name="android:layout_width">30dp</item>
        <item name="android:layout_height">30dp</item>
        <item name="android:scaleType">fitXY</item>
        <item name="android:background">@android:color/transparent</item>
    </style>
    <style name="TRTCVoiceRoomCreateRadioButtonStyle">
        <item name="android:button">@null</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">16sp</item>
    </style>
    <style name="TRTCVoiceRoomDialogTheme" parent="@android:style/Theme.Holo.Dialog">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowCloseOnTouchOutside">true</item>
    </style>
    <style name="TRTCVoiceRoomInputDialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowFullscreen">false</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:background">#00000000</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>
</resources>
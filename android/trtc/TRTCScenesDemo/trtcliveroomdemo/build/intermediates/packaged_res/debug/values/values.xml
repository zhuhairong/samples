    <style name="AppBaseTheme" parent="android:Theme.Black.NoTitleBar">
    </style>
    <style name="TRTCLiveRoomBeautyTheme" parent="BeautyPanelStyle">
        <item name="beautyPanelColorPrimary">@color/trtcliveroom_color_accent</item>
    </style>
    <style name="TRTCLiveRoomDialogFragment" parent="@android:style/Theme.Holo.Dialog">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowCloseOnTouchOutside">true</item>
    </style>
    <style name="TRTCLiveRoomDialogTheme" parent="Theme.AppCompat.DayNight.Dialog.Alert"/>
    <style name="TRTCLiveRoomInputDialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowFullscreen">false</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:background">#00000000</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>
    <style name="TRTCLiveRoomLinkMic">
        <item name="android:layout_width">120dp</item>
        <item name="android:layout_height">180dp</item>
        <item name="android:layout_marginEnd">8dp</item>
        <item name="android:layout_marginRight">8dp</item>
        <item name="android:layout_marginBottom">5dp</item>
    </style>
    <declare-styleable name="TRTCLiveRoomCustomTitle">
        <!-- 标题文案 -->
        <attr format="string" name="titleText"/>
        <!-- 是否有back按钮 -->
        <attr format="boolean" name="canBack"/>
        <!-- back按钮文案 -->
        <attr format="string" name="backText"/>
        <!-- 右侧更多功能按钮文字 -->
        <attr format="string" name="moreText"/>
    </declare-styleable>
    <declare-styleable name="TRTCLiveRoomHeartLayout">
        <attr format="dimension" name="initX"/>
        <attr format="dimension" name="initY"/>
        <attr format="dimension" name="xRand"/>
        <attr format="dimension" name="animLengthRand"/>
        <attr format="dimension" name="xPointFactor"/>
        <attr format="dimension" name="animLength"/>
        <attr format="dimension" name="heart_width"/>
        <attr format="dimension" name="heart_height"/>
        <attr format="integer" name="bezierFactor"/>
        <attr format="integer" name="anim_duration"/>
    </declare-styleable>
    <declare-styleable name="TRTCLiveRoomRoundImageView">
        <attr name="shape">
            <enum name="circle" value="0"/>
            <enum name="fillet" value="1"/>
        </attr>
        <attr format="dimension" name="radius"/>
        <attr format="color" name="cover_color"/>
    </declare-styleable>
